   #include<stdio.h>
#define n 3
void main(){
    int arr[n],i;
    for(i=0;i<n;i++){
    printf("\nEnter value %d : ",i+1);
    scanf("%d",&arr[i]);
    }
    for(i=(n-1);i>=0;i--){
        printf("%d ",arr[i]);
    }
}


    ---OUTPUT---
Enter value 1 : 1

Enter value 2 : 2

Enter value 3 : 3
3 2 1 
