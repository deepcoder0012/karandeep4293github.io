#include<stdio.h>
#define n 3
void main(){
    int arr[n],i;
    for(i=0;i<n;i++){
    printf("\nEnter value %d : ",i+1);
    scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++){
        if((arr[i]%2)==0)
        printf("%d ",arr[i]);
    }
}


    ---OUTPUT---
    Enter value 1 : 1

    Enter value 2 : 2
    
    Enter value 3 : 3
    2

    