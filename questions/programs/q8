#include<stdio.h>
#define n 3
void main(){
    int arr[n],i,max,min;
    for(i=0;i<n;i++){
    printf("\nEnter value %d : ",i+1);
    scanf("%d",&arr[i]);
    }
    max=arr[0];
    min=arr[0];
    for(i=1;i<n;i++){
        if(max<arr[i])
        max=arr[i];
        else
        min=arr[i];
    }
    printf("Min: %d Max: %d",min,max);
}


    ---OUtPUT---
Enter value 1 : 1

Enter value 2 : 2

Enter value 3 : 3
Min: 1 Max: 3

    