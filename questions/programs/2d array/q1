    
#include<stdio.h>
void main(){
    int matrix[4][2]={{1,2},
                      {3,4},
                      {5,6},
                      {7,8}};
    int r,c;
    for(r=0;r<4;r++){                   
    for(c=0;c<2;c++){
        printf("%d ",matrix[r][c]);
    }
    printf("\n");
    }                          
}

---OUTPUT---
1 2 
3 4 
5 6 
7 8
