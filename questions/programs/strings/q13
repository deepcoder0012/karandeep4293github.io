#include<stdio.h>
void main(){
    char str[]="kartik",cpyStr[sizeof(str)];
    int i=0;
    while(1){
        if(str[i]==0){
            cpyStr[i]=0;
            break;
        }else{
            cpyStr[i]=str[i];
        }
        i++;
    }
    printf("%s",cpyStr);
}


    ---OUTPUT---
    kartik

    