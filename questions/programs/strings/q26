#include<stdio.h>
void main(){
    char str[40];
    printf("\nEnter string : ");
    gets(str);
    int i;
    for(i=0;str[i]!='\0';i++){
        if(str[i]==' '){
            printf("Second word starts from %d index",i+1);
            break;
        }       
    }
}


    ---OUTPUT---
Enter string : hello world bye bye
Second word starts from 6 ind

    