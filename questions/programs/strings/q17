#include<stdio.h>
void main(){
    char str[]="kartik";
    int i,count=0;
    for(i=0;str[i]!=0;i++){
        if(str[i]=='a' || str[i]=='e' || str[i]=='i' || str[i]=='o' || str[i]=='u' || str[i]=='A' || str[i]=='E' || str[i]=='I' || str[i]=='O' || str[i]=='U'){
            count++;
        }
    }
    printf("There are %d vowels in string",count);
}


    ---OUTPUT---
    There are 2 vowels in string

    