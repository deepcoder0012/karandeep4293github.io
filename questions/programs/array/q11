#include<stdio.h>
#define n 4
void main(){
    int arr[n],i;
    for(i=0;i<n;i++){
    printf("\nEnter value %d : ",i+1);
    scanf("%d",&arr[i]);
    }
    for(int j=1;j<n;j++){
    for(i=j;i<n;i++){
        if(arr[j-1]==arr[i])
        printf("%d ",arr[i]);
    }
    }
}

    ---OUTPUT---
    Enter value 1 : 1

    Enter value 2 : 2
    
    Enter value 3 : 2
    
    Enter value 4 : 3
    2     

    